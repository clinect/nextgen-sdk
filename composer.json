{
    "name": "clinect/nextgen-sdk",
    "description": "Clinect NextGen Sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Clinect",
            "email": "support@clinecthealtcare.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "Clinect\\NextGenSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clinect\\NextGenSdk\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}