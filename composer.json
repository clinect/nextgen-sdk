{
    "name": "clinect/nextgen-sdk",
    "description": "Clinect NextGen Sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Clinect",
            "email": "support@clinecthealtcare.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/var-dumper": "^6.2",
        "illuminate/support": "^8.0",
        "sammyjo20/saloon": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Clinect\\NextGen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clinect\\NextGen\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clinect\\NextGen\\ServiceProvider"
            ],
            "aliases": {
                "NexGen": "Clinect\\NexGen\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
